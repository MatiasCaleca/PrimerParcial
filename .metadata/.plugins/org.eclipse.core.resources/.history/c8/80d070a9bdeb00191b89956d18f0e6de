#ifndef PEDIDOPENDIENTE_H_
#define PEDIDOPENDIENTE_H_

struct
{
	int idpedidoPendiente;
	int idCliente;
	int cantidadKilos;
	char estadoPedido;
	int isEmpty;

}typedef ePedidosPendiente;

int ePedidosPendienteInicializar(ePedidosPendiente ePedidosPendienteArray [], int size);

#endif /* PEDIDOPENDIENTE_H_ */
